{
  "name": "intelcommandconsole",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host localhost --port 5173",
    "start": "vite --host localhost --port 5173",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "patch-package",
    "wttp:install": "npm install wttp-handler @tw3/solidity",
    "wttp:compile": "npx hardhat compile",
    "wttp:deploy-contract": "npx hardhat run scripts/deploy-wttp-site.js",
    "wttp:deploy-content": "node scripts/wttp-deploy-content.cjs",
    "wttp:deploy": "npm run build && npm run wttp:deploy-content",
    "wttp:full-deploy": "npm run wttp:compile && npm run wttp:deploy-contract && npm run wttp:deploy",
    "wttp:test": "node scripts/wttp-test.cjs",
    "wttp:integration-test": "node scripts/wttp-integration-test.cjs",
    "wttp:prepare": "node scripts/wttp-prepare-content.cjs",
    "audit:knip": "knip --reporter markdown",
    "audit:depcheck": "depcheck --json",
    "audit:unimported": "unimported -f",
    "audit:madge": "madge --ts-config ./tsconfig.app.json --extensions ts,tsx --circular src || true",
    "audit:bundle": "npm run build && source-map-explorer 'dist/assets/*.js' --html dist/bundle-report.html || true",
    "audit:all": "npm run audit:knip && npm run audit:depcheck && npm run audit:unimported && npm run audit:madge",
    "lint:unused": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "samples:capture": "node scripts/capture-api-samples.js --source all",
    "samples:capture:noaa": "node scripts/capture-api-samples.js --source noaa",
    "samples:capture:usgs": "node scripts/capture-api-samples.js --source usgs",
    "samples:capture:coingecko": "node scripts/capture-api-samples.js --source coingecko",
    "samples:capture:reddit": "node scripts/capture-api-samples.js --source reddit",
    "samples:capture:hn": "node scripts/capture-api-samples.js --source hn",
    "samples:capture:github": "node scripts/capture-api-samples.js --source github"
  },
  "dependencies": {
    "@ensdomains/ensjs": "^4.0.2",
    "@metamask/eth-sig-util": "^8.2.0",
    "@tw3/solidity": "^0.1.5",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "crypto-js": "^4.2.0",
    "ethers": "^6.15.0",
    "ipfs-http-client": "^60.0.1",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "nanoid": "^5.1.5",
    "node-fetch": "^3.3.2",
    "pako": "^2.1.0",
    "parse-duration": "^2.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "wttp-handler": "^0.2.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@eslint/js": "^9.17.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.9",
    "@nomicfoundation/hardhat-ethers": "^3.0.9",
    "@nomicfoundation/hardhat-toolbox": "^6.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.13.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "chai": "^4.5.0",
    "cypress": "^14.5.1",
    "depcheck": "^1.4.7",
    "dotenv": "^17.2.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "hardhat": "^2.25.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "knip": "^5.62.0",
    "madge": "^8.0.0",
    "npm-force-resolutions": "^0.0.10",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "unimported": "^1.31.1",
    "vite": "^6.0.5"
  },
  "overrides": {
    "nanoid": "^5.1.5",
    "parse-duration": "^2.1.4",
    "cookie": "^0.7.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
